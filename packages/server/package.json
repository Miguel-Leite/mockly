{
  "name": "mockly",
  "version": "1.0.0",
  "description": "Mock APIs rapidinho, desenvolva sem esperar o backend",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mockly-server": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mock",
    "api",
    "server",
    "development",
    "frontend",
    "testing",
    "faker"
  ],
  "author": "Miguel Leite",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Miguel-Leite/mockly.git"
  },
  "bugs": {
    "url": "https://github.com/Miguel-Leite/mockly/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^10.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
